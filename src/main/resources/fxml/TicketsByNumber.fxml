<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXMasonryPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <VBox  spacing="10">

        <Label styleClass="title-label" text="Билеты по номерам">Flat Buttons</Label>
        <ScrollPane fitToWidth="true" fx:id="scrollPane" hbarPolicy="NEVER">
            <JFXMasonryPane fx:id="masonryPane" HSpacing="10" VSpacing="10">
                <padding><Insets top="16" right="16" bottom="16" left="16"/></padding>
            </JFXMasonryPane>
        </ScrollPane>
    </VBox>
</StackPane>
